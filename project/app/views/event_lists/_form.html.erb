<%= form_for(@event_list) do |f| %>
  <% if @event_list.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@event_list.errors.count, "error") %> prohibited this event_list from being saved:</h2>

      <ul>
      <% @event_list.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :eventDate, 'Event Date' %><br>
    <%= f.date_select :eventDate %>
  </div>
  <div class="field">
    <%= f.label :eventTime, 'Event Time' %><br>
    <%= f.time_select :eventTime %>
  </div>
  <div class="field">
    <%= f.label :eventName, 'Event Name' %><br>
    <%= f.text_field :eventName %>
  </div>
  <div class="field">
    <%= f.label :event_description, 'Event Description' %><br>
    <%= f.text_area :event_description %>
  </div>
	<div class="field">
    <%= f.label :nbrOfRiders, 'Rider Count' %><br>
    <%= f.number_field :nbrOfRiders %>
  </div>
  <div class="field">
    <%= f.label :vehicleType, 'Vehicle' %><br>
    <%= f.text_field :vehicle_type %>
  </div>
  <div class="field">
    <%= f.label :created_By, 'Created By' %><br>
  </div>
	<h3>Event Address</h3>
  <div class="field">
    <%= f.label :streetAddress, 'Address' %><br>
    <%= f.text_field :streetAddress %>
  </div>
  <div class="field">
    <%= f.label :City %><br>
    <%= f.text_field :City %>
  </div>
  <div class="field">
    <%= f.label :State %><br>
    <%= f.text_field :State %>
  </div>
  <div class="field">
    <%= f.label :Zip %><br>
    <%= f.text_field :Zip %>
  </div>
	<h3>Pick Up Address</h3>
  <div class="field">
    <%= f.label :puAddress, '(PU)Address' %><br>
    <%= f.text_field :puAddress %>
  </div>
  <div class="field">
    <%= f.label :puCity, '(PU) City' %><br>
    <%= f.text_field :puCity %>
  </div>
  <div class="field">
    <%= f.label :puState, '(PU) State' %><br>
    <%= f.text_field :puState %>
  </div>
  <div class="field">
    <%= f.label :puZip, '(PU) Zip' %><br>
    <%= f.text_field :puZip %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
